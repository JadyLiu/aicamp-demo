# Required environment variables for the DocAI Agent

# Mistral API configuration
MISTRAL_API_KEY=your_mistral_api_key_here

# Mistral Workflow API configuration
ABRAXAS_API_KEY=your_mistral_workflow_api_key_here
ABRAXAS_ENDPOINT=your_mistral_workflow_endpoint_here
TEMPORAL_NAMESPACE=your_temporal_namespace_here
ABRAXAS_ALLOW_OVERRIDE_NAMESPACE=true

# MongoDB configuration (optional - if not using hardcoded URI)
MONGO_URI=mongodb://localhost:27017
MONGO_DB_NAME=docai_demo
MONGO_COLLECTION_NAME=workflow_results
